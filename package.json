{
  "name": "hyperhubs-event-backend",
  "version": "0.0.1",
  "description": "Create,Manage,and Promote Events",
  "author": "hyperhubs",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lf": "npm run lint && npm run format",
    "launch": "npm run build && pm2 start dist/main.js --name hypberhubs"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.750.0",
    "@aws-sdk/credential-providers": "3.514.0",
    "@joi/date": "2.1.1",
    "@nestjs/common": "10.2.10",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.10",
    "@nestjs/event-emitter": "2.0.3",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mongoose": "10.0.2",
    "@nestjs/platform-express": "^10.4.15",
    "@types/bcryptjs": "2.4.6",
    "@types/compression": "1.7.5",
    "@types/express-useragent": "1.0.3",
    "@types/uuid": "9.0.5",
    "axios": "1.7.9",
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "dotenv": "16.3.1",
    "express-useragent": "1.0.15",
    "helmet": "7.0.0",
    "ioredis": "5.3.2",
    "joi": "17.10.2",
    "lodash": "4.17.21",
    "mongodb": "5.9.2",
    "mongoose": "6.12.0",
    "multer": "^1.4.5-lts.1",
    "nest-winston": "1.9.4",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "uuid": "9.0.1",
    "winston": "3.10.0",
    "winston-daily-rotate-file": "4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.2.1",
    "@nestjs/testing": "^11.0.11",
    "@types/express": "4.17.17",
    "@types/ioredis": "5.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "4.14.202",
    "@types/multer": "^1.4.12",
    "@types/node": "20.3.1",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.5.0",
    "prettier": "3.0.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  }
}
